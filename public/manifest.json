{
  "manifest_version": 3,
  "name": "Litcode Solver",
  "description": "An extension powered by Google Bard to solve litcode questions.",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://bard.google.com/"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png"
  }
}
